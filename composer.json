{
  "name": "zaengle/craft-readtime",
  "description": "Craft Read Time",
  "type": "craft-plugin",
  "authors": [
    {
        "name": "Zaengle Corp",
        "homepage": "https://zaengle.com/"
    }
  ],
  "require": {
    "php": "^8.0.1|^8.2",
    "craftcms/cms": "^4.0.1|^5.0.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "zaengle\\readtime\\": "src/"
    }
  },
  "extra": {
    "name": "Read Time",
    "handle": "readtime",
    "developer": "Zaengle",
    "documentationUrl": "",
    "class": "zaengle\\readtime\\ReadtimeField"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "^8.0.1|^8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}